{
    "name": "iter8/builder-bundle",
    "description": "Symfony bundle for builder",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Iter8\\Bundle\\BuilderBundle\\": ""
        }
    },
    "config": {
        "platform": {
            "php": "7.1.8"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "~7.1",
        "friendsofphp/php-cs-fixer": "^2.0",
        "iter8/builder": "^4.0",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0"
    }
}
